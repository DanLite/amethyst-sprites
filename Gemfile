source 'http://rubygems.org'

gem 'rails', '3.0.2'

# Bundle edge Rails instead:
# gem 'rails', :git => 'git://github.com/rails/rails.git'

gem 'sqlite3-ruby', :require => 'sqlite3'
gem 'paperclip'

gem 'aws-s3', :require => 'aws/s3'

gem 'devise'

group :development, :test do
  gem 'rspec'
  gem 'rspec-rails'
end

group :test do
  gem 'autotest'
  gem 'autotest-rails'
  gem 'factory_girl_rails'
end